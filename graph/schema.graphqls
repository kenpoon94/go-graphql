type User {
  _id: String!
  name: String!
  jobtitle: String!
  age: Int!
  city: String!
  hobbies: [String]!
}

type Query {
  user(_id: String!): User!
  users: [User]!
}

input NewUser {
  name: String!
  jobtitle: String!
  age: Int!
  city: String!
  hobbies: [String]!
}

type Mutation {
  createUser(input: NewUser): User!
}